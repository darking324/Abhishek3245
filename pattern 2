#include <stdio.h>

int main() {
    int rows, num = 0;

    printf("Enter number of rows: ");
    scanf("%d", &rows);

    for (int i = 1; i <= rows; i++) {
        for (int j = 1; j <= i; j++) {
            printf("%d", num);
            num++;  // Increment number for each printed value
        }
        printf("\n");
    }

    return 0;
}
