#include <stdio.h>

int main() {
    int n, fact = 1;

    // Input a number
    printf("Enter a positive integer: ");
    scanf("%d", &n);

    // Calculate factorial
    for (int i = 1; i <= n; i++)
        fact *= i;

    // Print result
    printf("Factorial of %d is %d\n", n, fact);

    return 0;
}
